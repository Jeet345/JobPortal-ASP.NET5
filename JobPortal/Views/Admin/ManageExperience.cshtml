@using Microsoft.AspNetCore.Http
@model IEnumerable<JobPortal.Models.JobSeekerExperience>

@{
    ViewData["Title"] = "Manage Experience";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<div class="content">


    <div class="add-btn">
        <Button class="btn">Add Record</Button>
    </div>

    <div class="display-table">

        <table class="table">

            <thead>
                <tr>
                    <th></th>
                    <th>Id</th>
                    <th>User Name</th>
                    <th>Experience</th>
                    <th>Designation</th>
                    <th>Organization</th>
                    <th>Starting Date</th>
                    <th>Ending Date</th>
                    <th>Salary</th>
                    <th>Job Profile</th>

                </tr>
            </thead>

            <tbody>


                @foreach (var item in Model)
                {
                    <tr>
                        <td>
                            <div class="action-btn">
                                <button class="btn edit-btn">Edit</button>
                                <button class="btn delete-btn">Delete</button>
                            </div>
                        </td>
                        <td>@item.Id</td>
                        <td>@item.JobSeeker.Name</td>
                        <td>@item.Experience</td>
                        <td>@item.Designation</td>
                        <td>@item.Organization</td>
                        <td>@item.StartMonth</td>
                        <td>@item.EndMonth</td>
                        <td>@item.Salary</td>
                        <td>@item.JobProfile</td>

                    </tr>
                }

            </tbody>

        </table>

    </div>

</div>

